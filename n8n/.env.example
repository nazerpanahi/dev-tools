# n8n Configuration
N8N_VERSION=1.90.2           # Docker image version to use
N8N_PORT=5678                # Port to expose on host
N8N_HOST=localhost           # Hostname where n8n can be reached
N8N_PROTOCOL=http            # Protocol to use (http or https)
N8N_ENCRYPTION_KEY=n8n-secure-encryption-key # Key to encrypt credentials
TZ=Asia/Tehran               # Timezone
WEBHOOK_URL=http://localhost:5678/ # URL used for webhooks
EXECUTIONS_PROCESS=main      # How to process executions (main or own)
N8N_DIAGNOSTICS_ENABLED=false # Send anonymous usage data
NODE_ENV=production          # Node environment

# Uncomment to enable basic authentication
# N8N_BASIC_AUTH_ACTIVE=true
# N8N_BASIC_AUTH_USER=admin
# N8N_BASIC_AUTH_PASSWORD=secure-password 